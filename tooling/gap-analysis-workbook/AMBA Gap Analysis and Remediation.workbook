    },
    {
      "type": 3,
      "content": {
        "version": "KqlItem/1.0",
        "query": "policyresources\r\n| where type == \"microsoft.authorization/policysetdefinitions\"\r\n| where id has 'managementGroups/{managementGroup}'\r\n| mv-expand amba = properties.metadata._deployed_by_amba\r\n| where amba == \"True\"\r\n| project Name=name, id\r\n| join kind=leftouter (\r\n    policyresources\r\n    | where type =~ \"microsoft.authorization/policyassignments\"\r\n    | mv-expand properties.policyDefinitionId\r\n    | extend policyDefID = tostring(properties_policyDefinitionId)) on $left.id == $right.policyDefID\r\n| project Name, id, AssignmentID=id1",
        "size": 1,
        "title": "Custom AMBA Initiatives Deployed by this Workbook",
        "exportedParameters": [
          {
            "fieldName": "id",
            "parameterName": "policySetID",
            "parameterType": 1
          },
          {
            "fieldName": "Name",
            "parameterName": "policySetName",
