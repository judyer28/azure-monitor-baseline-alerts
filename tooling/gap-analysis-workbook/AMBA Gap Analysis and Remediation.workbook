            "value": "AMBA LAW Initiative"
          },
          {
            "id": "28fd8e0f-0467-4860-b6c4-361295a051b0",
            "version": "KqlParameterItem/1.0",
            "name": "initiativeAssignmentName",
            "type": 1,
            "query": "resources\r\n| limit 1\r\n| extend initiativeAssignmentName = substring(replace_string(\"{initiativeName}\",\"/\",\" \"), 0, 23)\r\n| project initiativeAssignmentName",
            "crossComponentResources": [
              "value::tenant"
