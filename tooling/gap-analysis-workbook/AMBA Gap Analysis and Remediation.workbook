      "name": "text - 22"
    },
    {
      "type": 3,
      "content": {
        "version": "KqlItem/1.0",
        "query": "policyresources\r\n| where type =~ \"microsoft.authorization/policydefinitions\"\r\n| extend alertGUID = \"{alertGUID}\"\r\n| where id has alertGUID\r\n| mv-expand properties.displayName\r\n| project Name=properties_displayName, id, alertGUID\r\n| join kind=leftouter (\r\n    policyresources\r\n    | where type =~ \"microsoft.authorization/policyassignments\"\r\n    | mv-expand properties.policyDefinitionId\r\n    | extend policyDefID = tostring(properties_policyDefinitionId)) on $left.id == $right.policyDefID\r\n| project Name, PolicyID=id, alertGUID, AssignmentID=id1 ",
        "size": 4,
        "noDataMessage": "No policy definitions found for this alert",
        "queryType": 1,
        "resourceType": "microsoft.resources/tenants",
        "crossComponentResources": [
