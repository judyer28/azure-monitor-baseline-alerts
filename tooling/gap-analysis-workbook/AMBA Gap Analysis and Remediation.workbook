        "query": "resources\r\n| extend rg = strcat('/subscriptions/', subscriptionId, '/resourcegroups/', resourceGroup)\r\n| where rg in~ ({resourceGroups})\r\n| where type =~ \"{Type}\"\r\n| extend alertTable = pack({alertList})\r\n| where array_index_of(bag_keys(alertTable), type) != -1\r\n| extend alerts = split(alertTable[type], \",\")\r\n| extend alertCount = array_length(alerts)\r\n| mv-expand alerts\r\n| where alerts =~ \"{metricName}\"\r\n| project id, name, type=tolower(type), location, resourceGroup, subscriptionId, tostring(alerts), alertCount\r\n| join kind=leftouter  (\r\n    resources\r\n    | extend rg = strcat('/subscriptions/', subscriptionId, '/resourcegroups/', resourceGroup)\r\n    | where rg in~ ({resourceGroups})\r\n    | where type =~ \"{Type}\"\r\n    | summarize numResourcesByType=count() by type\r\n    ) on $left.type == $right.type\r\n| project-away type1\r\n| join kind=leftouter  (\r\n    resources\r\n    | extend rg = strcat('/subscriptions/', subscriptionId, '/resourcegroups/', resourceGroup)\r\n    | where rg in~ ({resourceGroups})\r\n    | where type =~ 'microsoft.insights/metricalerts'\r\n    | extend scopes = properties.scopes \r\n    | mv-expand scopes\r\n    | extend metricName = tostring(properties.criteria.allOf[0].metricName)\r\n    | where metricName =~ \"{metricName}\"\r\n    | extend targetType = tolower(properties.targetResourceType)\r\n    | extend targetRegion = tostring(properties.targetResourceRegion)\r\n    | project metricName, targetType, targetRegion, scopes\r\n    ) on $left.alerts == $right.metricName\r\n| where ((type =~ targetType) or (isempty(targetType))) and ((location =~ targetRegion) or (isempty(targetRegion)))\r\n| extend isSubScope = iff(array_length(split(scopes,'/'))==3, true, false)\r\n| extend subMatch = iff(isSubScope == true and scopes == strcat('/subscriptions/', subscriptionId), true, false)\r\n| extend isRgScope = iff(array_length(split(scopes,'/'))==5, true, false)\r\n| extend rgMatch = iff(isRgScope == true and scopes == strcat('/subscriptions/', subscriptionId, '/resourcegroups/', resourceGroup), true, false)\r\n| extend idMatch = iff(id =~ scopes, true, false)\r\n| extend implemented = iff(subMatch or rgMatch or idMatch, true, false)\r\n| summarize implementedList=make_list(implemented) by id, name, type, location, resourceGroup, subscriptionId, missingAlert=tostring(alerts), alertCount, numResourcesByType\r\n| where array_index_of(implementedList,true) == -1\r\n| project id, name, resourceGroup, location, missingAlert\r\n| extend rgID = (split(id, \"/providers\"))[0]\r\n| project id, resourceGroup, location, Deploy=\"Deploy Alert\", rgID, alertName=strcat(\"AMBA-\",tostring(missingAlert),\"-\",resourceGroup,\"-\",name), depGUID=new_guid()\r\n| extend subscriptionId = (split(id,'/'))[2]\r\n| extend depCLICmd = strcat('az account set --subscription ',subscriptionId,' && az deployment group create --no-wait --name \"AMBA-',depGUID,'\" --resource-group \"',resourceGroup,'\" --template-uri \"{armTemplateURL}\" --parameters alertName=\"',alertName,'\" targetResourceId=\"',id,'\" targetResourceRegion=\"',location,'\" targetResourceType=\"{Type}\"')",
